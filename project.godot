config_version=3

[application]

config/name="BASE-6"
run/main_scene="res://Scenes/Startup.tscn"
boot_splash/image="res://Sprites/Splash.png"
config/icon="res://icon.png"

[autoload]

PlayerNoise="*res://Globals/PlayerNoise.tscn"
Screencap="*res://Globals/Screencap.tscn"
audioplayer="*res://Globals/audioplayer.tscn"
Player="*res://Globals/Player.tscn"
controller="*res://Scripts/Globals/controller.gd"

[display]

window/size/width=160
window/size/height=144
window/size/resizable=false
window/size/test_width=960
window/size/test_height=864
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[importer_defaults]

texture={
